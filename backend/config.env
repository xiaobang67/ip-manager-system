# 开发环境配置文件
# 请根据实际情况修改配置

# 数据库配置
DB_HOST=mysql
DB_PORT=3306
DB_USER=ipuser
DB_PASSWORD=ippassword
DB_NAME=ip_management_system

# 连接池配置
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30

# API配置
API_HOST=0.0.0.0
API_PORT=8000
DEBUG_MODE=True

# JWT配置 (如果需要认证)
JWT_SECRET_KEY=your_jwt_secret_key_here_change_in_production
JWT_ALGORITHM=HS256
JWT_EXPIRE_HOURS=24

# CORS配置
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080", "http://localhost"]

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log