# 数据库配置示例
# 复制此文件为 config.env 并修改相应配置

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=ip_management_system

# 连接池配置
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30

# API配置
API_HOST=0.0.0.0
API_PORT=8000
DEBUG_MODE=True

# JWT配置 (如果需要认证)
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRE_HOURS=24

# CORS配置
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log